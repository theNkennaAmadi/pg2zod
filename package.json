{
  "name": "pg-to-zod",
  "version": "1.0.0",
  "type": "module",
  "description": "Introspect PostgreSQL databases and generate strict, comprehensive Zod v4 schemas with full type coverage",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "pg-to-zod": "./dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "node --test dist/**/*.test.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "postgresql",
    "postgres",
    "zod",
    "schema",
    "validation",
    "introspection",
    "database",
    "typescript",
    "codegen"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.16.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
